# --- gRPC Client Configuration for Development ---

# Server Address
CHAT_SERVER_ADDRESS=127.0.0.1:8080

# TLS/Security (Set to false for local dev)
CHAT_SERVER_USE_TLS=false
CHAT_SERVER_CERT_FILE=

# Connection/Call Behavior
CHAT_SERVER_TIMEOUT=5s
CHAT_SERVER_MAX_RETRIES=0
CHAT_SERVER_KEEPALIVE=false

# gRPC Service Configuration
CHAT_SERVER_ENABLE_RETRY=false
CHAT_SERVER_MAX_MESSAGE_SIZE=104857600